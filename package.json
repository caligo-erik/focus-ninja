  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.19.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
